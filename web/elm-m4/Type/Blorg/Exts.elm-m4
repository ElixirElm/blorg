import Date exposing(..)
IMPORT_JSON_DECODE

decodeDate =
  Decode.customDecoder Decode.string Date.fromString

decodeNullOr decoder =
  Decode.oneOf
    [ Decode.null Nothing
    , Decode.map Just decoder
    ]
